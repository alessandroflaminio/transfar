<Window x:Class="Transfar.ReceivingFileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Transfar"
        mc:Ignorable="d"
        Title="ReceivingFileWindow" Height="300" Width="300">
    <Grid>
        <Button x:Name="yesButton" Content="Yes" HorizontalAlignment="Left" Margin="34,198,0,0" VerticalAlignment="Top" Width="75" Click="Yes_Button_Click"/>
        <Button x:Name="noButton" Content="No" HorizontalAlignment="Left" Margin="179,198,0,0" VerticalAlignment="Top" Width="75" Click="No_Button_Click"/>
    </Grid>
</Window>
