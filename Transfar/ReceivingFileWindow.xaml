<Window x:Class="Transfar.ReceivingFileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Transfar"
        mc:Ignorable="d"
        Title="ReceivingFileWindow" Height="300" Width="300">
    <Grid>
        <Button x:Name="yesButton" Content="Yes" Margin="30,198,187,0" VerticalAlignment="Top" Click="Yes_Button_Click"/>
        <Button x:Name="noButton" Content="No" Margin="187,198,30,0" VerticalAlignment="Top" Click="No_Button_Click"/>
        <ProgressBar x:Name="progressBar" Height="31" Margin="10,109,10,0" VerticalAlignment="Top" Visibility="Hidden"/>
        <Button x:Name="cancelButton" Content="Cancel transfer" Margin="96,198,96,0" VerticalAlignment="Top" Click="Cancel_Button_Click" Visibility="Hidden"/>
    </Grid>
</Window>
